<%- include("partials/header"); -%>
<h1>Compose</h1>
<form class="" action="" method="post">
  <div class="mb-3" style="padding-top:20px;">
    <label class="form-label">Title</label>
    <input type="text" class="form-control" name="titleName" value="">
  </div>
  <div class="mb-3" style="padding-top:20px;">
    <label class="form-label">Post</label>
    <textarea name="titleBody" rows="8" cols="80" class="form-control"></textarea>
  </div>
  <div class="md-3" style="padding-top:20px;">
    <button  type="submit" name="button" class="btn btn-primary">Publish</button>
  </div>
</form>

